{
  "manifest_version": 3,
  "name": "Your Extension Name",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "webRequest",
    "webNavigation"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "48x48.png",
    "128": "128x128.png"
  }
}
